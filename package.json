{
  "name": "cdk-assignment",
  "description": "CDK assignment and sample application",
  "license": "UNLICENSED",
  "private": true,
  "engines": {
    "node": ">=22.0.0"
  },
  "scripts": {
    "build:app": "tsc -p tsconfig.json",
    "build:cdk": "tsc -p cdk/tsconfig.json",
    "build": "npm run build:app && npm run build:cdk",
    "start": "node dist/app/index.js",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/ test/ cdk/",
    "format": "prettier --write .",
    "fix": "npm run lint -- --fix && npm run format",
    "dev": "ts-node-dev --respawn --transpile-only --ignore-watch test,dist src/index.ts",
    "cdk": "cdk --app \"node dist/cdk/bin/cdk-assignment.js\""
  },
  "devDependencies": {
    "@eslint/js": "^9.24.0",
    "@tsconfig/node22": "^22.0.1",
    "@types/express": "^5.0.1",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.14.1",
    "@types/supertest": "^6.0.3",
    "aws-cdk": "^2.1015.0",
    "eslint": "^9.24.0",
    "eslint-config-prettier": "^10.1.2",
    "eslint-plugin-jest": "^28.11.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^5.2.6",
    "eslint-plugin-security": "^3.0.1",
    "jest": "^29.7.0",
    "prettier": "^3.5.3",
    "supertest": "^7.1.0",
    "ts-jest": "^29.3.2",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.30.1"
  },
  "dependencies": {
    "aws-cdk-lib": "^2.196.0",
    "constructs": "^10.4.2",
    "express": "^5.1.0"
  }
}
